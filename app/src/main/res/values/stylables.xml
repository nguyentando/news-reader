<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolbarView">
        <attr name="tv_title" format="string" />
        <attr name="show_back" format="boolean" />
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="AspectRatioImageView">
        <attr name="ari_aspectRatio" format="string" />
        <attr name="ari_aspectRatioEnabled" format="boolean" />
        <attr name="ari_dominantMeasurement">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>
</resources>